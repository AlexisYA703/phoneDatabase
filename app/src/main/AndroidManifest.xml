<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dynnamicdevz.storageapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.StorageApp">
        <activity android:name=".main.view.view.PhoneDetailsActivity"></activity>
        <activity
            android:name=".main.view.view.SellPhone"
            android:label="@string/title_activity_sell_phone"
            android:theme="@style/Theme.StorageApp.NoActionBar" />
        <activity
            android:name=".main.view.view.PhoneSQLiteDatabase"
            android:label="@string/title_activity_phone_sqlite_database"
            android:theme="@style/Theme.StorageApp.NoActionBar" />
        <activity android:name=".main.view.view.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>